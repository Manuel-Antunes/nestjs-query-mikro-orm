{
  "name": "nestjs-query-mikro-orm",
  "version": "0.0.5",
  "description": "NestJS Query adapter for MikroORM",
  "author": "Manuel Antunes",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build && pnpm run test"
  },
  "dependencies": {
    "camel-case": "^4.1.2",
    "lodash.filter": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@mikro-orm/core": "^6.6.3",
    "@mikro-orm/knex": "^6.6.3",
    "@mikro-orm/nestjs": "^6.0.2",
    "@mikro-orm/sqlite": "^6.6.3",
    "@nestjs-query/core": "^0.30.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@swc/core": "^1.10.6",
    "@types/lodash.filter": "^4.6.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/lodash.omit": "^4.5.9",
    "@types/node": "^22.13.5",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitest/coverage-v8": "^3.0.0",
    "class-transformer": "^0.5.1",
    "esbuild": "^0.23.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.1",
    "prettier": "^3.5.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@mikro-orm/core": "^6.0.0",
    "@mikro-orm/nestjs": "^6.0.0",
    "@nestjs-query/core": "^0.30.0",
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "class-transformer": "^0.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Manuel-Antunes/nestjs-query-mikro-orm.git"
  },
  "keywords": [
    "nestjs",
    "nestjs-query",
    "mikro-orm",
    "orm",
    "query",
    "graphql"
  ]
}